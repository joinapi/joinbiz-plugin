{
    "name": "joinapi/joinbiz-plugin",
    "description": "JOINBIZ App Plugin",
    "keywords": [
        "joinapi",
        "laravel",
        "joinbiz-plugin"
    ],
    "homepage": "https://github.com/joinapi/joinbiz-plugin",
    "support": {
        "issues": "https://github.com/joinapi/joinbiz-plugin/issues",
        "source": "https://github.com/joinapi/joinbiz-plugin"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rodrigo Souza",
            "email": "devops@joinapi.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^3.0",
        "joinapi/data-accounting": "^1.0",
        "joinapi/data-common": "^1.0",
        "joinapi/data-party": "^1.0",
        "joinapi/data-security": "^1.0",
        "laravel/framework": "^11.9",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^9.0",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^3.0.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Joinbiz\\BizApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joinbiz\\BizApp\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joinbiz\\BizApp\\BizAppServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
